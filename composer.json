{
    "name": "brunocfalcao/crypto-crawler-binance",
    "description": "Nidavellir crypto crawler for Binance",
    "keywords": ["Crypto", "Binance"],
    "homepage": "https://github.com/brunocfalcao/crypto-crawler-binance",
    "license": "MIT",
    "authors": [{
        "name": "Bruno Falcao",
        "email": "bruno.c.falcao@brunofalcao.me",
        "homepage": "https://www.brunocfalcao.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "brunocfalcao/crypto-cube": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\Crawler\\Binance\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\Crawler\\Binance\\CryptoCrawlerBinanceServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
